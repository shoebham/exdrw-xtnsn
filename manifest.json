{
  "manifest_version": 3,
  "name": "Excalidraw in New Tab",
  "version": "1.0",
  "description": "Opens Excalidraw in every new tab",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
  
}
